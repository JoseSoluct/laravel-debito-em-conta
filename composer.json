{
    "name": "josea/laravel-debito-em-conta",
    "description": "Pacote para geração de arquivos de débito em conta",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Josea\\LaravelDebitoEmConta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josea\\LaravelDebitoEmConta\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "José Alcides",
            "email": "jose.alcides@soluct.com.br"
        }
    ],
    "require": {
      "ext-mbstring": "*",
        "php": "^8.0",
        "neitanod/forceutf8": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "friendsofphp/php-cs-fixer": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
